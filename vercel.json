{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://yourprojectdomain.com",

        "APP_STORAGE": "/tmp/storage",

        "VIEW_COMPILED_PATH": "/tmp/storage/framework/views",

        "APP_CONFIG_CACHE": "/tmp/storage/bootstrap/cache/config.php",
        "APP_EVENTS_CACHE": "/tmp/storage/bootstrap/cache/events.php",
        "APP_PACKAGES_CACHE": "/tmp/storage/bootstrap/cache/packages.php",
        "APP_ROUTES_CACHE": "/tmp/storage/bootstrap/cache/routes.php",
        "APP_SERVICES_CACHE": "/tmp/storage/bootstrap/cache/services.php",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",

        "APP_KEY": "YOUR_ACTUAL_APP_KEY_HERE"
    },
    "installCommand": "composer install --no-dev --optimize-autoloader && php artisan config:clear && php artisan cache:clear && php artisan view:clear",
    "buildCommand": "php artisan optimize"
}
